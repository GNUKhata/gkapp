{
  "name": "gkapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-dev": "vue-cli-service build --mode development",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "i18n-report": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/locales/**/*.?(json|yml|yaml|js)'",
    "i18n-add-missing": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/locales/**/*.?(json|yml|yaml|js)' --add true",
    "i18n-remove-unused": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/locales/**/*.?(json|yml|yaml|js)' --remove true"
  },
  "dependencies": {
    "bootstrap-vue": "^2.21.2",
    "check-password-strength": "^1.0.15",
    "core-js": "^3.19.1",
    "crypto-js": "^4.1.1",
    "html2canvas": "^1.3.2",
    "jspdf": "^2.4.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.14",
    "vue-i18n": "^8.26.3",
    "vue-router": "^3.5.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.11.5",
    "@intlify/vue-i18n-loader": "^1.1.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-pwa": "^4.5.15",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-vuex": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "axios": "^0.21.4",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "mutationobserver-shim": "^0.3.7",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "prettier": "^1.19.1",
    "sass": "^1.43.4",
    "sass-loader": "^10.2.0",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-bootstrap-vue": "~0.7.0",
    "vue-cli-plugin-i18n": "~2.3.1",
    "vue-i18n-extract": "^2.0.4",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
